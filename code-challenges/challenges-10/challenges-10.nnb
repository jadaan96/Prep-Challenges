            "source": [
                "function compareTriplets(a, b) {\n    let arr =[]\n    let x=0\n    let w =0\n    for ( let i = 0 ; i<a.length ; i++)\n    if( a[i] > b[i]) {\n        x++\n    }else if (a[i] < b[i]){\n        w++\n    }\n   arr.push(x)\n   arr.push(w)\n   return arr\n}"
